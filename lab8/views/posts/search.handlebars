<h1>{{title}}</h1>
<h2>{{personName}}</h2>
<main class = "people">
<ul>
	{{#each people}}
	<li>
		<a href="/details/{{this.id}}">{{this.firstName}} {{this.lastName}}</a><br>
		{{/each}}
	</li>
</ul>
</main>
{{#unless people.length}}
<p class="not-found">No results found for {{personName}}.</p>
{{/unless}}

<br>
<br>

<a href = "/" id="back_to_search">Back to search</a>
